		var isSelfTeam:Boolean = $assisterCtrler.getSelf().team.id == sp.team.id;
		if (isSelfTeam) {
			continue;
		}
		
		var direct:int = sp.direct==selfDirect ? -1:1;
		sp.speedLevel = speedLevel * direct;
	}
}

