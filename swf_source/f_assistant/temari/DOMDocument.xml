function fuutonFunc():void {
	setTargetBulletSpeedLevel(level);
	
	var targetState:int = $assisterCtrler.owner_fighter_ctrler.getTargetState();
	if (targetState == 23) {
		return;
	}
	
	targetSP.x += finalSpeed * selfDirect;
}
function setTargetBulletSpeedLevel(speedLevel:Number = 0):void {
	var gameSprites:Vector.<*> = $assisterCtrler.getGameSprites();
	for each(var sp:* in gameSprites) {
		if (!$assisterCtrler.isBullet(sp)) {
			continue;
